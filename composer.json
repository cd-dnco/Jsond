{
    "name": "cd-dnco/jsond",
    "description": "Adds Twig filters for working with json.",
    "type": "craft-plugin",
    "version": "1.06",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "jsond"
    ],
    "support": {
        "docs": "https://github.com/cd-dnco/Jsond/blob/master/README.md",
        "issues": "https://github.com/cd-dnco/Jsond/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "The Indigo Viking",
            "homepage": "https://www.theindigoviking.com"
        },
        {
            "name": "Chris Dekker",
            "homepage": "https://dnco.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "indigoviking\\jsond\\": "src/"
        }
    },
    "extra": {
        "name": "Json'd",
        "handle": "jsond-dnco",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/cd-dnco/Jsond/blob/master/CHANGELOG.md",
        "class": "indigoviking\\jsond\\Jsond"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
